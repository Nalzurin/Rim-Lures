      <drawSize>2.85</drawSize>
      <shadowData>
        <volume>(1.6875, 0.375, 2.125)</volume>
        <offset>(0,0,-0.12)</offset>
      </shadowData>
      <texPath>Things/Building/Misc/SmartPheromoneLauncher</texPath>
